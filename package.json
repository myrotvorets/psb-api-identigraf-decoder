{
  "name": "identigraf-decoder-microservice",
  "version": "2.0.2",
  "type": "module",
  "private": true,
  "description": "IDentigraF OID Decoder Microservice",
  "scripts": {
    "wait-for-mysql": "wait-on \"tcp:$KNEX_HOST:3306\" -t 180000",
    "build": "tsc",
    "start:dev": "nodemon -L",
    "lint": "eslint -f gha .",
    "lint:fix": "eslint --fix .",
    "typecheck": "tsc --noEmit",
    "pretest": "npm run lint",
    "test": "mocha test",
    "pretest:coverage": "npm run build",
    "test:coverage": "rm -rf dist/src/specs && cp -R src/specs dist/src/ && JS_TESTS=1 c8 mocha dist/test",
    "posttest": "npm run build",
    "preci": "npm run build && npm run wait-for-mysql && cp -R src/specs dist/src/ && SEED_TABLES=yes node ./dist/test/migrate.mjs",
    "ci": "JS_TESTS=1 c8 mocha dist/test/integration"
  },
  "author": "Myrotvorets <support@myrotvorets.center> (https://myrotvorets.center/)",
  "license": "MIT",
  "dependencies": {
    "@cloudnative/health-connect": "^2.1.0",
    "@myrotvorets/envalidators": "^2.1.0",
    "@myrotvorets/express-async-middleware-wrapper": "^2.2.0",
    "@myrotvorets/express-microservice-middlewares": "^2.2.0",
    "@myrotvorets/express-request-logger": "^1.2.1",
    "@myrotvorets/oav-installer": "^5.0.0",
    "@myrotvorets/opentelemetry-configurator": "^7.2.0",
    "@myrotvorets/opentelemetry-plugin-knex": "^0.30.1",
    "@myrotvorets/otel-utils": "^1.1.0",
    "@opentelemetry/api": "^1.7.0",
    "@opentelemetry/core": "^1.18.1",
    "@opentelemetry/semantic-conventions": "^1.18.1",
    "awilix": "^9.0.0",
    "envalid": "^8.0.0",
    "express": "^4.18.2",
    "express-openapi-validator": "^5.1.0",
    "knex": "^2.5.1",
    "mysql2": "^3.6.5"
  },
  "devDependencies": {
    "@myrotvorets/eslint-config-myrotvorets-ts": "^2.24.0",
    "@myrotvorets/fake-knex-client": "^1.1.1",
    "@types/chai": "^4.3.11",
    "@types/chai-as-promised": "^7.1.8",
    "@types/express": "^4.17.21",
    "@types/mocha": "^10.0.6",
    "@types/mock-knex": "^0.4.8",
    "@types/node": "^20.10.0",
    "@types/supertest": "^2.0.16",
    "c8": "^8.0.1",
    "chai": "^4.3.10",
    "chai-as-promised": "^7.1.1",
    "eslint-formatter-gha": "^1.4.3",
    "eslint-plugin-mocha": "^10.2.0",
    "mocha": "^10.2.0",
    "mocha-multi": "^1.1.7",
    "mocha-reporter-gha": "^1.1.1",
    "mocha-reporter-sonarqube": "^2.0.5",
    "mock-knex": "^0.4.12",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2",
    "wait-on": "^7.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/myrotvorets/psb-api-identigraf-decoder.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/myrotvorets/psb-api-identigraf-decoder/issues"
  },
  "homepage": "https://github.com/myrotvorets/psb-api-identigraf-decoder#readme"
}
